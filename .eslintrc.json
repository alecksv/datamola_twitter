{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "airbnb/hooks"
    ],
   
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": [
            "error",
            {
                "allow": [
                    "foo_",
                    "_bar"
                ]
            }
        ]
        
    }
}

{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "airbnb/hooks"
    ],
   
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": ["off"],
        "linebreak-style":["off"],
        "max-classes-per-file":["off"],
        "no-shadow":["off"],
        "max-len":["off"],
        "no-console":["off"],
        "no-unused-vars":["off"],
        "valid-typeof":["off"],
        "no-plusplus":["off"],
        "consistent-return":["off"],
        "array-callback-return":["off"],
        "no-param-reassign":["off"],
        "no-unreachable-loop":["off"]
        
    }
}
